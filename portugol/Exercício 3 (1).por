programa {
  funcao inicio() {
    real n1, n2, consumo_medio

    escreva("Digite a distância total do percurso: ")
    leia(n1)

    escreva("Digite o total de combustível gasto: ")
    leia(n2)
    
    consumo_medio = (n1/n2)
    
    escreva("O consumo médio do automóvel é: ",consumo_medio)

  }
}
